{
  "name": "react-wrapper-mapkitjs",
  "version": "0.0.11",
  "description": "Modern react wrapper for apple mapkitjs",
  "files": [
    "dist"
  ],
  "main": "dist/r-wrapper-mapkitjs.umd.js",
  "module": "./dist/r-wrapper-mapkitjs.es.js",
  "exports": {
    ".": {
      "import": "./dist/r-wrapper-mapkitjs.es.js",
      "require": "./dist/r-wrapper-mapkitjs.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avocadoteam/react-mapkitjs.git"
  },
  "keywords": [
    "react",
    "mapkitjs"
  ],
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "author": "avocadoteam",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/little-loader": "^0.2.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "chalk": "^4.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "execa": "^5.0.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "semver": "^7.3.7",
    "typescript": "^4.7.3",
    "vite": "^2.9.12"
  },
  "dependencies": {
    "little-loader": "^0.2.0"
  }
}
